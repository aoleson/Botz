{
  "manifest_version": 2,

  "name": "Botz",
  "description": "Something to do with robots on the new tab page. It's gonna be great",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  
  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  }
}